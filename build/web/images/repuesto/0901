		<script type="text/javascript">

			function prepareUrl(url, suffixToRemove, suffixToAdd) {
			    if (!url) {
			        return;
                }
				suffixToRemove = suffixToRemove ? suffixToRemove : '';
				suffixToAdd = suffixToAdd ? '/' + suffixToAdd + '/' : '';
				var source = 'amayama.com/' + suffixToRemove;
				return url.replace(source, 'amayama.com' + suffixToAdd).replace(/#[\w\d]+$/, '');
			}

			function prepareNeedle($needle) {
                var data = $needle.mouseout().data('maphilight') || {};
                data.alwaysOn = true;
                $needle.data('maphilight', data).trigger('alwaysOn.maphilight');
                jQuery(detailListSelector + ' a[href*="' + prepareUrl($needle.attr('href')) + '"]')
                    .css('background', linkBackgroundColor)
                    .addClass('freeze');

                            }

			var linkBackgroundColor = '#f9c981';
			var detailListSelector = '.detail-list';
			jQuery(function () {
				jQuery('img[usemap]').maphilight({
					fillColor: '008800'
				});
				jQuery(detailListSelector + ' a').mouseover(function(e) {
					jQuery('area[href*="' + prepareUrl(jQuery(this).attr('href'), 'ru') + '"]').mouseover();
				}).mouseout(function(e) {
					jQuery('area[href*="' + prepareUrl(jQuery(this).attr('href'), 'ru') + '"]').mouseout();
				});
				jQuery('area').mouseover(function(e) {
					jQuery(detailListSelector + ' a[href*="' + prepareUrl(jQuery(this).attr('href'), '', 'ru') + '"]')
						.css('background', linkBackgroundColor)
				}).mouseout(function(e) {
					jQuery(detailListSelector + ' a[href*="' + prepareUrl(jQuery(this).attr('href'), '', 'ru') + '"]:not(.freeze)')
						.css('background', 'transparent');
				});
							});
		</script>
	
